MODULE set_ts_tab_0100 OUTPUT.
  CASE ts_tab-activetab.
    WHEN 'TAB1'.
      gv_dynnr = '103'.
    WHEN 'TAB2'.
      gv_dynnr = '104'.
    WHEN OTHERS. "Default tab 설정
      ts_tab-activetab = 'TAB1'.
      gv_dynnr = '103'.

  ENDCASE.
ENDMODULE.